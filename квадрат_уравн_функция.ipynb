    "def func(a,b,c):\n",
    "    d=b**2 - 4*a*c\n",
    "    if d == 0:\n",
    "        return (- b / (2*a))\n",
    "    elif d < 0:\n",
    "        return None\n",
    "    else:\n",
    "        return  ((-b + np.sqrt(d)/(2*a)),(-b - np.sqrt(d))/(2*a))"
