    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "a = np.zeros(100)\n",
    "a[0] = np.log((1 + 10)/(10))\n",
    "c = 1\n",
    "while c!=100:\n",
    "    a[c] = (1 - (10)*c*a[c-1])/c\n",
    "    c+=1"
